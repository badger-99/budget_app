<p style="color: green"><%= notice %></p>

<h1>Categories</h1>
<%= button_to "Sign Out", destroy_user_session_path, method: :get %>

<div id="categories">
  <% @categories.each do |category| %>
    <div>
      <i class="<%= category.icon %>"></i>
      <div>
        <h3><%=category.name%><h3>
        <p>$ 000.00</p>
      </div>
    </div>
  <% end %>
</div>

<%= link_to "New category", new_category_path %>
